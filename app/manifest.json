{
    "name": "Twicker",
    "description": "Twicker - a twitter newspicker.",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": ["scripts/background.js"]
        }
    },
    "permissions": [
        "alwaysOnTopWindows",
        "https://userstream.twitter.com/*",
        "https://api.twitter.com/*",
        "https://userstream.twitter.com/1.1/user.json",
        "https://api.twitter.com/oauth/authorize",
        "https://api.twitter.com/oauth/request_token",
        "https://api.twitter.com/oauth/access_token",
        "https://oauth.io/auth/*",
        "identity",
        "webview",
        "https://google.com/*",
        "https://nljnhhhpfjakmgehjhifghpkmeomjhgh.chromiumapp.org/*",
        "https://mfmobhfajifmehiejhkbmobmkhfflgdf.chromiumapp.org/*"
    ],
    "icons": { "16": "images/calculator-16.png", "128": "images/calculator-128.png" }
}