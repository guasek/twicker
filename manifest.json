{
    "name": "Twicker",
    "description": "Twicker - a twitter newspicker.",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": ["background.js", "bar.js"]
        }
    },
    "permissions": ["alwaysOnTopWindows", "https://userstream.twitter.com/*", "https://api.twitter.com/*", "identity"],
    "icons": { "16": "calculator-16.png", "128": "calculator-128.png" }
}